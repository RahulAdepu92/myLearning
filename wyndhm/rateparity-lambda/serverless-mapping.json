{
    "template": {
        "compiled": "cloudformation-template-update-stack.json",
        "core": "cloudformation-template-create-stack.json"
    },
    "stack": "WHG-CF-USW2-$service-$stage",
    "role": {
        "Fn::Join": [
            "-", [
                "WHG",
                "$service",
                "$stage",
                "$region",
                "LambdaRole"
            ]
        ]
    },
    "policy": {
        "Fn::Join": [
            "-", [
                "WHG",
                "$service",
                "lambda",
                "$stage"
            ]
        ]
    },
    "lambda": "WHG-Lambda-USW2-$service$lambda-$stage",
    "logGroup": "/aws/lambda/WHG-Lambda-USW2-$service$lambda-$stage"
}